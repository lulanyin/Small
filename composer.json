{
  "name": "lulanyin/small",
  "description": "PHP网站服务开发框架",
  "keywords": [
    "small",
    "php framework"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "lulanyin",
      "email": "me@lanyin.lu"
    }
  ],
  "require": {
    "lulanyin/db": "dev-master",
    "lulanyin/smarty": "dev-master",
    "easyswoole/verifycode": "^2.0",
    "intervention/image": "^2.4",
    "codeitnowin/barcode": "^3.0",
    "doctrine/annotations": "1.4",
    "ext-json": "^1.5",
    "ext-curl": "^7.1",
    "php": "^7.1",
    "ext-pcntl": "^7.1",
    "ext-mbstring": "^7.1",
    "ext-iconv": "^7.1",
    "ext-exif": "^7.1"
  },
  "require-dev": {
    "swoft/swoole-ide-helper": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Small\\" : "src/",
      "app\\" : "app/"
    },
    "files": [
      "src/helpers/functions.php",
      "src/helpers/shortcut.php"
    ]
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.laravel-china.org"
    }
  }
}
