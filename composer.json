{
  "name": "lulanyin/small",
  "description": "PHP网站服务开发框架",
  "keywords": [
    "small",
    "php framework"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "lulanyin",
      "email": "me@lanyin.lu"
    }
  ],
  "require": {
    "lulanyin/smarty": "dev-master",
    "doctrine/annotations": "1.4",
    "ext-pdo": "*",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-mbstring": "*",
    "ext-iconv": "*",
    "php": "^7.2"
  },
  "require-dev": {
    "swoft/swoole-ide-helper": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Small\\" : "src/"
    },
    "files": [
      "src/Helpers/functions.php",
      "src/Helpers/shortcut.php"
    ]
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
  }
}
